{
    "reactApp": {
        "src": "./src/app/",
        "extname": "jsx",
        "build": "./build/js/"
    },
    "styles": {
        "css":{
            "src": "./src/styles/",
            "extname": "css"
        },
        "sass": {
            "src": "./src/styles/",
            "extname": "scss"
        },
        "build": "./build/css/"
    },
    "script": {
        "src": "./src/js",
        "build": "./build/js/"
    },
    "vendor": {
        "require": [
        "react"
        ],

      "build": {
          "dirname": "./build/js/",
          "name": "vendor.js"
      }
    }
}
